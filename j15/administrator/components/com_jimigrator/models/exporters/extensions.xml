<?xml version="1.0" encoding="utf-8"?>
<exporter version="1.5">
	<name>Extensions</name>
	<description>Export Extensions</description>
	<group>joomlacore</group>
    <tables>
        <table name="extensions" pkey="extension_id" />
    </tables>
	<files>
		<filename>extensions.csv</filename>
	</files>
	<fields>
		<field type="hidden" default="1" name="runorder" />
		<field type="hidden" default="extensions" name="globalvalues" />
	</fields>
</exporter>