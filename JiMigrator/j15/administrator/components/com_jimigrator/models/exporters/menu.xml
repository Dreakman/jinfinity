<?xml version="1.0" encoding="utf-8"?>
<exporter version="1.5">
	<name>menu</name>
	<description>Export Menu Items</description>
	<group>joomlacore</group>
    <tables>
        <table name="menu" pkey="id" />
    </tables>
	<files>
		<filename>menu.csv</filename>
	</files>
	<fields>
        <field type="checkbox" default="0" name="selective" label="Only Export Selected Items" description="Only export the items specified in the list filter below. When this option is unchecked all items will be exported" />
        <field type="menufilter" default="0" name="filter" label="Menu Filter" description="Selective export filter" class="jitogglertab jidjifields_menuselective" />
	</fields>
</exporter>