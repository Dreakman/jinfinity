<?xml version="1.0" encoding="utf-8"?>
<importer version="1.6">
	<name>KunenaForum</name>
	<description>Import Kunena Forum</description>
    <group>thirdparty</group>
    <!-- >>> PRO >>> -->
    <tables>
        <table name="kunena_aliases" pkey="alias" />
        <table name="kunena_announcement" pkey="id" />
        <table name="kunena_attachments" pkey="id" />
        <table name="kunena_categories" pkey="id"  />
        <table name="kunena_config" pkey="id" />
        <table name="kunena_config_backup" pkey="id" />
        <table name="kunena_configuration" pkey="id" />
        <table name="kunena_favorites" pkey="id" />
        <table name="kunena_groups" pkey="id" />
        <table name="kunena_keywords" pkey="id" />
        <table name="kunena_keywords_map" />
        <table name="kunena_messages" pkey="id" />
        <table name="kunena_messages_text" pkey="mesid" />
        <table name="kunena_messages_ranks" pkey="rank_id" />
        <table name="kunena_moderation" pkey="id" />
        <table name="kunena_polls" pkey="id" />
        <table name="kunena_polls_options" pkey="id" />
        <table name="kunena_polls_users" pkey="id" />
        <table name="kunena_ranks" pkey="id" />
        <table name="kunena_sessions" pkey="userid" />
        <table name="kunena_smileys" pkey="id" />
        <table name="kunena_subscriptions" pkey="id" />
        <table name="kunena_subscriptions_categories" />
        <table name="kunena_thankyou" pkey="id" />
        <table name="kunena_topics" pkey="id" />
        <table name="kunena_users" pkey="userid" />
        <table name="kunena_users_banned" pkey="id" />
        <table name="kunena_user_categories" />
        <table name="kunena_user_read" />
        <table name="kunena_user_topics" />
        <table name="kunena_version" pkey="id" />
        <table name="kunena_whoisonline" pkey="id" />
    </tables>
    <!-- <<< PRO <<< -->
    <fields>
        <!-- >>> PRO >>> -->
		<field type="list" default="0" name="truncate" label="Clear DB Tables" description="Clear several Kunena Forum database tables before starting the import process">
			<option value="1">Yes</option>
			<option value="0">No</option>
		</field>
		<field type="list" default="0" name="overwrite" label="Overwrite Existing" description="Items in the database with the same ID will be replaced">
			<option value="1">Yes</option>
			<option value="0">No</option>
		</field>
        <field type="list" default="0" name="append" label="Append If Exists" description="If an item exists in the database with the same ID then the new item will be appended to the end">
            <option value="1">Yes</option>
            <option value="0">No</option>
        </field>
		<field type="separator" label="Debug" />
		<field type="list" default="0" name="debug" label="Debug Run" description="No database changes will be made. Check the log for what changes would have occurred">
			<option value="1">Yes</option>
			<option value="0">No</option>
		</field>
		<field type="list" default="0" name="debuglevel" label="Debug Level" description="More log entries will be made for higher debug levels">
			<option value="0">Standard</option>
			<option value="1">Development</option>
		</field>
        <!-- <<< PRO <<< -->
        <!-- >>> FREE >>> -->
        <field type="separator" label="JI_ONLY_AVAILABLE_IN_PRO_TITLE" />
        <!-- <<< FREE <<< -->
	</fields>
</importer>
