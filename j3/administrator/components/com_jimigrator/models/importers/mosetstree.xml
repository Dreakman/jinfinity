<?xml version="1.0" encoding="utf-8"?>
<importer version="1.6">
    <name>MosetsTree</name>
    <description>Import Mosets Tree</description>
    <group>thirdparty</group>
    <!-- >>> PRO >>> -->
    <tables>
        <table name="mt_archived_log" pkey="log_id" />
        <table name="mt_archived_reviews" pkey="rev_id" />
        <table name="mt_archived_users" pkey="id" />
        <table name="mt_cats" pkey="cat_id" />
        <table name="mt_cfvalues" pkey="id" />
        <table name="mt_cfvalues_att" pkey="att_id" />
        <table name="mt_cl" pkey="cl_id" />
        <table name="mt_claims" pkey="claim_id" />
        <table name="mt_clone_owners" pkey="user_id" />
        <table name="mt_config" pkey="varname" />
        <table name="mt_configgroup" pkey="groupname" />
        <table name="mt_customfields" pkey="cf_id" />
        <table name="mt_favourites" pkey="fav_id" />
        <table name="mt_fields_map" />
        <table name="mt_fieldtypes" pkey="ft_id" />
        <table name="mt_images" pkey="img_id" />
        <table name="mt_linkcheck" pkey="id" />
        <table name="mt_links" pkey="link_id" />
        <table name="mt_links_associations" />
        <table name="mt_log" pkey="log_id" />
        <table name="mt_relcats" />
        <table name="mt_reports" pkey="report_id" />
        <table name="mt_reviews" pkey="rev_id" />
        <table name="mt_templates" pkey="tem_id" />
    </tables>
    <!-- <<< PRO <<< -->
    <fields>
        <!-- >>> PRO >>> -->
        <field type="list" default="0" name="truncate" label="Clear DB Tables" description="Clear several Mosets Tree database tables before starting the import process">
            <option value="1">Yes</option>
            <option value="0">No</option>
        </field>
        <field type="list" default="0" name="overwrite" label="Overwrite Existing" description="Items in the database with the same ID will be replaced">
            <option value="1">Yes</option>
            <option value="0">No</option>
        </field>
        <field type="list" default="0" name="append" label="Append If Exists" description="If an item exists in the database with the same ID then the new item will be appended to the end">
            <option value="1">Yes</option>
            <option value="0">No</option>
        </field>
        <field type="separator" label="Debug" />
        <field type="list" default="0" name="debug" label="Debug Run" description="No database changes will be made. Check the log for what changes would have occurred">
            <option value="1">Yes</option>
            <option value="0">No</option>
        </field>
        <field type="list" default="0" name="debuglevel" label="Debug Level" description="More log entries will be made for higher debug levels">
            <option value="0">Standard</option>
            <option value="1">Development</option>
        </field>
        <!-- <<< PRO <<< -->
        <!-- >>> FREE >>> -->
        <field type="separator" label="JI_ONLY_AVAILABLE_IN_PRO_TITLE" />
        <!-- <<< FREE <<< -->
    </fields>
</importer>